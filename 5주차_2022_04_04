a=9 #a는 5를 가리킵니다.
while a<9:
    print (f'a is {a:10}');
    a+=1 #a= a-1 과 동일한코드 
        
# 8단을 출력하는 코드를 작성 하시오
a=8
b=1 
while a<72:
    a=8*b
    print (f'8X{b}={a}');
    b+=1


#문제 숫자판 출력 

    
c=int(input('정수를 입력 하시오 : ')) #입력받은 데이터가 문자이끼 때문에 정수로 밖굼
a=1
b=0
#n 이  a의 제곡보다 작은 동안 반복 
while a<=(c**2):
    
    print(f'{a:2}',end='');
    if a%c==0:
             print ();
    elif a%c==0:
             print ();     

    a+=1 ;  #n은 +1 증강
        
